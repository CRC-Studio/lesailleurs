
<!--
Le fichier loco.xml permet de supporter la traduction d'élément
situé dans le code du template

  Exemple d'HTML :
    <?php _e("OK","lesailleurs") ?>
 -->


<?xml version="1.0" encoding="utf-8"?>
<bundle name="lesailleurs">
  <domain name="lesailleurs">
    <project name="lesailleurs" slug="lesailleurs">
      <target>
        <directory>languages</directory>
      </target>
      <template>
        <file>languages/lesailleurs.pot</file>
      </template>
    </project>
  </domain>
</bundle>
